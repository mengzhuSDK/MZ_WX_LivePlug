<!--pages/index/index.wxml-->
<view class="test-page">
    <view>请输入直播id号（ticketId）：</view>
    <input type="text" placeholder="默认为: 10181510" class="test-inp" bindinput="ticketId" />
    <view>请输入用户id（uniqueId）：</view>
    <input type="text" placeholder="为空则默认为: mengzhuyouke" class="test-inp" bindinput="uniqueId" />
    <view>请输入用户昵称（nickName）：</view>
    <input type="text" placeholder="为空则默认为: 游客" class="test-inp" bindinput="nickName" />
    <view>请输入用户手机号（phone）：</view>
    <input type="text" placeholder="为空则默认为: 19912344321" class="test-inp" bindinput="phone" />
    <view>请输入用户头像（avatarUrl）：</view>
    <view class="info">*必须使用 https 开头</view>
    <input type="text" placeholder="https://s1.zmengzhu.com/user/images/user-default-image.png" class="test-inp" bindinput="avatarUrl" />

    <view class="toPlayerView">
        <button class="landspacePlayerView" bind:tap="toLandspacePlayerView" type="primary" >横屏播放</button>
        <button class="portraitPlayerView" bind:tap="toPortraitPlayerView" type="primary" >竖屏播放</button>
    </view>
   <view class="pusher-View">
        <button class="pusher-btn" bindtap="toLivePuhser" type="primary">测试推流</button>
   </view>

</view>
